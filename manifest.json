{
  "manifest_version": 2,
  "name": "2day is a Good Day",
  "description": "Yay, Today will be a good day",
  "version": "0.2.1",
  "icons": {
    "16": "icon32.png",
    "48": "icon64.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon64.png",
    "default_title": "Say Hi to your Today"
  },
  "chrome_url_overrides" : {
    "newtab": "2day.html"
  },
  "permissions": [
    "http://*/",
    "tabs"
  ]
}
